{
  "manifest_version": 2,
  "name": "URL2QR",
  "version": "0.1.1",
  "description": "Generate QR code from URL.",
  "icons": {
    "16": "/icons/icon32.png",
    "48": "/icons/icon96.png",
    "128": "/icons/icon256.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "page": "/html/background.html"
  },
  "browser_action": {
    "default_icon": "/icons/icon38.png",
    "default_title": "URL2QR",
    "default_popup": "/html/popup.html"
  },
  "offline_enabled": false,
  "author": "1000ch"
}